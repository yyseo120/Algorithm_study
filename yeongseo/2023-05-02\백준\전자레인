time= int(input())
if time%10==0:
    if time//300:
        a = time//300
        time = time - a*300
    else:
        a=0
    if time//60:
        b = time//60
        time = time - b*60
    else:
        b=0
    if time//10:
        c = time//10
        time = time - c*10
    else:
        c=0
    print(a, b, c)
else:
    print(-1)
